{
  "name": "hightracks",
  "version": "1.0.0",
  "private": true,
  "main": "src/main.js",
  "scripts": {
    "start": "node src/main",
    "test": "jest --forceExit --reporters=default --reporters=jest-junit",
    "coverage": "jest --forceExit --reporters=default --reporters=jest-junit --coverage --coverageReporters=text --coverageReporters=html --coverageDirectory=./coverage/",
    "lint": "eslint src/**/*.js -f checkstyle -o coverage/eslint-result.xml",
    "lint:json": "eslint src/**/*.js --format json -o coverage/eslint-result.json --debug"
  },
  "jest-junit": {
    "outputDirectory": "./coverage/",
    "outputName": "test.results.xml",
    "usePathForSuiteName": "true"
  },
  "jest": {
    "moduleNameMapper": {
      "axios": "axios/dist/node/axios.cjs"
    }
  },
  "dependencies": {
    "axios": "^1.3.5",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^16.0.3",
    "express": "~4.18.2",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^5.13.17",
    "node-fetch": "^3.3.1",
    "nodemon": "^2.0.22",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^4.6.2"
  },
  "devDependencies": {
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.5.1",
    "jest-junit": "^13.0.0",
    "prettier": "^2.5.1",
    "supertest": "^6.2.2"
  }
}